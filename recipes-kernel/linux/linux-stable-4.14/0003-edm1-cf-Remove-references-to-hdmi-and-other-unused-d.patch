-
+*/
 	mxcfb1: fb@0 {
 		compatible = "fsl,mxc_sdc_fb";
 		disp_dev = "ldb";
@@ -168,7 +170,7 @@
 		late_init = <0>;
 		status = "disabled";
 	};
-
+/*
 	mxcfb2: fb@1 {
 		compatible = "fsl,mxc_sdc_fb";
 		disp_dev = "hdmi";
@@ -200,7 +202,7 @@
